[
	{ kernel,
		[
			{start_dist_ac, false},
			{inet_dist_listen_min, 9000},
			{inet_dist_listen_max, 9000}
		]
	},
    {main_ifs,
        [
			{tls_conf,  "main_ifs-0.1.0/ssl.conf"}
        ]
    },
    {mnesia,
        [    
            {dir, "emod_esnmp-0.2.0/priv/mnesia"}
        ]
    },
    {snmp,
        [
            {manager,
                [
                    {versions, [v1,v2,v3]},
                    {config,
                        [
                            {dir, "emod_esnmp-0.2.0/priv/snmp/manager"},
                            {db_dir, "emod_esnmp-0.2.0/priv/snmp/manager"}
                            %{verbosity, debug}
                        ]
                    },
                    {def_user_mod,  esnmp_user_default},
                    {def_user_data, esnmp_user_default},
                    {net_if,
                            [
                                {module, esnmp_net_if}
                                %{verbosity, trace}
                            ]
                    }
                ]
            },
            {agent,
                [
                    {config, [{dir, "emod_esnmp-0.2.0/priv/snmp/agent"}]},
                    {db_dir, "emod_esnmp-0.2.0/priv/snmp/agent"},
                    {versions, [v3]}
                ]
            }
        ]
    }
].
