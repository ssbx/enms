<?xml version="1.0" encoding="UTF-8"?>
<snmp_def>
    <descr>Intefaces performances</descr>
    <type>table</type>
    <performances>
    <!-- hard coded in lib/monitor/src/snmp_ifPerf.erl
        <data_sources>
            <ds>
                <dsName>OctetsIn</dsName>
                <dsType>COUNTER</dsType>
            </ds>
            <ds>
                <dsName>OctetsOut</dsName>
                <dsType>COUNTER</dsType>
            </ds>

            <ds>
                <dsName>UnicastPacketsIn</dsName>
                <dsType>COUNTER</dsType>
            </ds>
            <ds>
                <dsName>UnicastPacketsOut</dsName>
                <dsType>COUNTER</dsType>
            </ds>

            <ds>
                <dsName>NonUnicastPacketsIn</dsName>
                <dsType>COUNTER</dsType>
            </ds>
            <ds>
                <dsName>ErrorsIn</dsName>
                <dsType>COUNTER</dsType>
            </ds>
            <ds>
                <dsName>ErrorsOut</dsName>
                <dsType>COUNTER</dsType>
            </ds>

        </data_sources>
    -->
        <graphs>
            <graph_desc>
                <graphName>TCPLatency</graphName>
                <graphTitle>TCP Performances</graphTitle>
                <verticalLabel>Milliseconds</verticalLabel>
                <min>0</min>
                <max>undefined</max>
                <rigid>false</rigid>
                <base>1000</base>
                <unit>ms</unit>
                <unitExponent>0</unitExponent>
                <add>
                    <name>ReplyDuration</name>
                    <graphType>line</graphType>
                    <legend>Max reply latency</legend>
                    <consolFun>MAX</consolFun>
                    <color>#729FCFFF</color>
                </add>
                <add>
                    <name>ReplyDuration</name>
                    <graphType>line</graphType>
                    <legend>Average reply latency</legend>
                    <consolFun>AVERAGE</consolFun>
                    <color>#204A87FF</color>
                </add>
                <add>
                    <name>ReplyDuration</name>
                    <graphType>line</graphType>
                    <legend>Min reply latency</legend>
                    <consolFun>MIN</consolFun>
                    <color>#3665A4FF</color>
                </add>
            </graph_desc>
        </graphs>
    </performances>
</snmp_def>
